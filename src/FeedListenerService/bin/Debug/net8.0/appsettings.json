{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "FeedListenerService": "Information"
    }
  },
  "AllowedHosts": "*",
  "Kafka": {
    "BootstrapServers": "kafka:9092",
    "Topics": {
      "IngestEvents": "ingest-events",
      "LiveScores": "live-scores",
      "PlayerUpdates": "player-updates",
      "UserNotifications": "user-notifications"
    }
  },
  "FeedGenerator": {
    "EventGenerationIntervalMs": 100,
    "NumberOfMatches": 20,
    "HighThroughputMode": true
  }
}